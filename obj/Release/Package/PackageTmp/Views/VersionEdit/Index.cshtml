@model IEnumerable<PressFitApi.Models.Version>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IOSVersion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AndriodVersion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ForceFulUpdate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IOSVersion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AndriodVersion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ForceFulUpdate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
